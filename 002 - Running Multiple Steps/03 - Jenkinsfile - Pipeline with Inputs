pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
	paramenters {
	string(name: 'NAME')
	string(name: 'AGE')
	}
    stages {
        stage('build') {
            steps {
                echo "${params.NAME}"
                echo "$AGE"
            }
        }
    }
}